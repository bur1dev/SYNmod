{
  "name": "syntext",
  "version": "0.0.1",
  "scripts": {
    "build": "rollup -c",
    "dev": "concurrently -k \"npm run dev:libs\" \"npm run start:happ\" \"rollup -c -w\"",
    "start": "sirv public",
    "start:happ": "cd ../../../ && npm run build:happ && npm run start:happ",
    "dev:libs": "cd ../../../ && npm run build:watch -w @syn/elements"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@syn/elements": "0.0.1",
    "@syn/store": "0.0.1",
    "concurrently": "^6.2.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@holochain-open-dev/cell-client": "^0.0.3",
    "@holochain/conductor-api": "0.2.0",
    "sirv-cli": "^1.0.0",
    "svelte-fa": "^2.2.0"
  }
}
